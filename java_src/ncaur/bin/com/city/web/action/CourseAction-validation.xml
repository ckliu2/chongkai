<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>



   <field name="course.stime">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>

    <field name="course.no">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="course.courseName">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="course.teacher">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="registerBegin">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="registerEnd">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    
    <field name="course.typeId">
        <field-validator type="required">
            <message key="errors.required"></message>
        </field-validator>
    </field>
</validators>
